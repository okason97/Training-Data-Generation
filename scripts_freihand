#!/bin/bash
python src/main.py -t -hdf5 -batch_stat -metrics is fid prdc -ref "valid" -data /mnt/data/datasets/freihand/ -cfg ./src/configs/FreiHAND/SPADE.yaml -save ./samples/FreiHAND/SPADE/ --is_ref_dataset --project titan --num_workers 4 -every 100 --pose > ./logs/FreiHAND/SPADE/run0.out 2> ./logs/FreiHAND/SPADE/run0.err
# nohup python src/main.py -sf -sf_num 1000 -ref "valid" -data ./data/rwth/ -cfg ./src/configs/RWTH/SPADE.yaml -save ./samples/RWTH/SPADE/ -ckpt ./samples/RWTH/SPADE/checkpoints/RWTH-SPADE-train-2022_06_16_00_13_05/ --num_workers 4 --truncation_factor 0.5 --pose > ./logs/RWTH/SPADE/sf0.out 2> ./logs/RWTH/SPADE/sf0.err
# nohup python src/main.py -sf -sf_num 1000 -ref "valid" -data ./data/rwth/ -cfg ./src/configs/RWTH/SPADE-ReACGAN.yaml -save ./samples/RWTH/SPADE-ReACGAN/ -ckpt ./samples/RWTH/SPADE-ReACGAN/checkpoints/RWTH-SPADE-ReACGAN-train-2022_06_16_06_50_26/ --num_workers 4 --truncation_factor 0.5 --pose > ./logs/RWTH/SPADE-ReACGAN/sf0.out 2> ./logs/RWTH/SPADE-ReACGAN/sf0.err
# nohup python src/main.py -sf -sf_num 1000 -ref "valid" -data ./data/rwth/ -cfg ./src/configs/RWTH/ReACGAN-ADC-DiffAug.yaml -save ./samples/RWTH/ReACGAN-ADC-DiffAug/ -ckpt ./samples/RWTH/ReACGAN-ADC-DiffAug/checkpoints/RWTH-ReACGAN-ADC-DiffAug-train-2022_06_16_15_02_50/ --num_workers 4 --truncation_factor 0.5 --pose > ./logs/RWTH/ReACGAN-ADC-DiffAug/sf0.out 2> ./logs/RWTH/ReACGAN-ADC-DiffAug/sf0.err
# python src/main.py -t -hdf5 -l -mpc -metrics is fid prdc -ref "valid" -data ./data/rwth/ -cfg ./src/configs/RWTH/StyleGAN2-D2DCE-DiffAug.yaml -save ./samples/RWTH/StyleGAN2-D2DCE-DiffAug/ --is_ref_dataset --project titan --num_workers 4 -every 500 > ./logs/RWTH/StyleGAN2-D2DCE-DiffAug/run0.out 2> ./logs/RWTH/StyleGAN2-D2DCE-DiffAug/run0.err